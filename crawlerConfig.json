{
  "index_name": "suzhibin",
  "start_urls": [
    "https://blog.suzhibin.cn/sitemap.xml"
  ],
  "sitemaps": [
    "https://blog.suzhibin.cn/sitemap.xml"
  ],
  "force_sitemap_urls_crawling": true,
  "stop_urls": [
    "/tags/",
    "/categories/",
    "/page/",
    "/pages/"
  ],
  "selectors": {
    "lvl0": {
      "selector": "",
      "default_value": "博客文章"
    },
    "lvl1": "#app .main-layout .post h1",
    "lvl2": "#app .main-layout .post h2",
    "lvl3": "#app .main-layout .post h3",
    "lvl4": "#app .main-layout .post h4",
    "category": "#app .main-layout .post .cat-item span",
    "tags": "#app .main-layout .post .tag-item span",
    "content": "#app .main-layout .post .markdown-main-style"
  },
  "selectors_exclude": [
    ".main-header",
    ".footer-link",
    ".main-footer",
    ".home"
  ],
  "js_render": true,
  "js_wait": 2,
  "max_depth": 0
}
