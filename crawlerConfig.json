{
  "index_name": "suzhibin",
  "start_urls": ["https://blog.suzhibin.cn"],
  "rateLimit": 8,
  "maxDepth": 10,
  "selectors": {
    "lvl0": { "selector": ".post .post-meta .title h1", "defaultValue": "Documentation" },
    "lvl1": ".post h1",
    "lvl2": ".post h2",
    "lvl3": ".post h3",
    "lvl4": ".post h4",
    "lvl5": ".post h5",
    "content": ".post p, .post li"
  },
  "selectors_exclude": [
    ".main-header", ".footer-link", ".main-footer", ".home"
  ],
  "js_render": true
}
